config_version=5

[application]

config/name="MMOscop"
run/main_scene="res://scenes/rooms/garalina.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="MMOscop"
config/features=PackedStringArray("4.1", "GL Compatibility")
run/max_fps=30
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://gift.png"

[audio]

general/3d_panning_strength=2.0

[autoload]

Global="*res://scripts/global.gd"
Console="*res://scenes/objects/menu/console.tscn"
bg_music="*res://scenes/objects/setup/bg_music.tscn"
