[sub_resource type="GDScript" id="GDScript_v5jfi"]
script/source = "extends Node3D

var timer:float = 0

func _process(delta):
	timer += delta
	
	rotation.z = -sin(1 * timer * PI) * cos(timer * PI / 5) * 0.25
	rotation.y = -cos(1 * (timer + 0.25) * PI) * sin(timer * PI / 5) * 0.4

	timer += delta
"

