script/source = "extends Sprite3D

var toggle = false
var timer_start = false
func _process(_delta):
	if get_tree().get_first_node_in_group(\"HUD_textboxes\").get_child(0).get_child_count()>0 && global_position.distance_to(get_tree().get_first_node_in_group(\"Player\").global_position)<2.5:
		if !timer_start:
			$Timer.start()
			timer_start = true
	else:
		if get_tree().get_first_node_in_group(\"HUD_textboxes\").get_child(0).get_child_count()==0 && global_position.distance_to(get_tree().get_first_node_in_group(\"Player\").global_position)<2.5:
			if self.frame_coords.x%2!=0:
				self.frame_coords.x-=1
		$Timer.stop()
		timer_start = false
	if get_parent().interacted:
		if self.frame_coords.x==0:
			self.frame_coords.x=2

func _on_timer_timeout():
	if self.frame_coords.x%2==0:
		self.frame_coords.x+=1
	else:
		self.frame_coords.x-=1


func _on_song_finished():
	bg_music.increase_volume()
"

